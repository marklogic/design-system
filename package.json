  "scripts": {
    "test": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "prepare": "npm run minify-css && npm run build",
    "predeploy": "cd playground && yarn && npm run build",
    "deploy": "gh-pages -d playground/build",
    "install:all": "(yarn link; pushd playground; yarn link \"marklogic-ui-library\"; popd); (yarn); (cd playground && yarn)",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -c .storybook -o storybook-dist",
    "minify-css": "node-sass --include-path scss src/styles.scss dist/styles.min.css --output-style compressed"
  },
  "peerDependencies": {
    "antd": "^3.20.5",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
