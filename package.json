{
  "author": "MarkLogic",
  "name": "@marklogic/design-system",
  "description": "MarkLogic Engineering UX design system",
  "version": "0.2.2",
  "license": "Apache-2.0",
  "repository": "http://github.com/marklogic/design-system",
  "main": "lib/index.js",
  "module": "es/index.js",
  "engines": {
    "node": ">=10",
    "npm": ">=5"
  },
  "sideEffects": [
    "dist/*",
    "es/**/style/*",
    "lib/**/style/*",
    "*.css",
    "*.less"
  ],
  "scripts": {
    "build": "gulp compile-all",
    "watch": "gulp compile-watch",
    "test": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd playground && yarn && yarn run build",
    "prepublishOnly": "yarn run build",
    "deploy": "gh-pages -d playground/build",
    "install:all": "(yarn unlink; yarn link; pushd playground; yarn link @marklogic/design-system; popd); (yarn); (cd playground && yarn); (yarn run build)",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "gulp compile-bundle-less; build-storybook -c .storybook -o storybook-dist"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-external-helpers": "^7.2.0",
