    "@storybook/addon-docs": "^6.0.0-beta.19",
    "@storybook/addon-info": "^6.0.0-alpha.2",
    "@storybook/addon-knobs": "^6.0.0-beta.19",
    "@storybook/addon-links": "^6.0.0-beta.19",
    "@storybook/addon-storyshots": "6.0.0-beta.19",
    "@storybook/addons": "^6.0.0-beta.19",
    "@storybook/react": "^6.0.0-beta.19",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/react": "^10.0.4",
    "babel-eslint": "^10.1.0",
