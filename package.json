    "gulp-debug-streams": "^3.0.0",
    "gulp-dedupe": "^0.0.2",
    "gulp-eslint": "^6.0.0",
    "gulp-eval": "^1.0.0",
    "gulp-exec": "^5.0.0",
    "gulp-less": "^4.0.1",
    "gulp-rename": "^2.0.0",
    "jest-specific-snapshot": "^3.0.0",
    "jest-transform-stub": "^2.0.0",
